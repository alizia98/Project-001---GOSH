    "def summerize_data(pc_df):\n",
    "    for column in pc_df.columns:\n",
    "        print (column)\n",
    "        if pc_df.dtypes[column] == np.object: # Categorical data\n",
    "            print (pc_df[column].value_counts())\n",
    "        else:\n",
    "            print (pc_df[column].describe()) \n",
    "        \n",
    "        print ('\\n')\n",
    "    \n",
    "summerize_data(pc_df)"
   ]
